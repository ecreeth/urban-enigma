{
  "name": "urbanenigma",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bundle": "yarn bundle:ios && yarn bundle:android",
    "bundle:ios": "react-native rnx-bundle --platform ios --dev false --reset-cache",
    "bundle:android": "react-native rnx-bundle --platform android --dev false --reset-cache"
  },
  "dependencies": {
    "react": "18.0.0",
    "react-native": "0.69.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.0",
    "@rnx-kit/cli": "^0.14.1",
    "@rnx-kit/metro-config": "^1.2.36",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.21",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnx-kit": {
    "bundle": {
      "treeShake": true,
      "entryFile": "index.js",
      "targets": [
        "ios",
        "android"
      ]
    }
  }
}
